{
  "builds": [
    {
      "src": "dtb/main.py",
      "use": "@vercel/python"
    },
    {
      "src": "build.sh",
      "use": "@vercel/static-build",
      "config": { "distDir": "staticfiles" }
    }
  ],
  "routes": [
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/(.*)",
      "dest": "dtb/wsgi.py"
    }
  ],
  "outputDirectory": "staticfiles"
}